{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2022",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "noEmit": false
  }
}
